{
  "name": "messaging",
  "version": "1.0.0",
  "description": "Challenge",
  "author": "FJ Caballero",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "migration:create": "NODE_ENV=local yarn run typeorm -- migration:create -n",
    "migration:generate": "NODE_ENV=local yarn run typeorm -- migration:generate -n",
    "setup:local": "NODE_ENV=local yarn run typeorm -- migration:run",
    "setup:test": "NODE_ENV=test yarn run typeorm -- migration:run",
    "test": "NODE_ENV=test jest -i",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv-safe": "8.2.0",
    "express": "^4.18.2",
    "express-promise-router": "4.1.1",
    "helmet": "6.0.1",
    "http-status": "1.6.2",
    "joi": "17.8.3",
    "morgan": "1.10.0",
    "pg": "8.9.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "4.1.3",
    "typeorm": "0.2.45",
    "winston": "3.8.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/dotenv-safe": "8.1.2",
    "@types/express": "4.17.17",
    "@types/helmet": "0.0.48",
    "@types/jest": "29.4.0",
    "@types/morgan": "1.9.4",
    "@types/node": "^18.15.0",
    "@types/supertest": "2.0.12",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "jest": "29.4.3",
    "nodemon": "2.0.20",
    "prettier": "2.8.4",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.14.2",
    "typescript": "4.9.5"
  }
}
